    "def url_list(page1_url):\n",
    "    var_url = '&section_offset=5&items_offset='\n",
    "    url_lst = [page1_url]\n",
    "    for page in range(1,17):\n",
    "        url = page1_url+var_url+str(18*page)\n",
    "        url_lst.append(url)\n",
    "    return url_lst"
   ]
  }
